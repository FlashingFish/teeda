<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config>
	<lifecycle>
		<phase-listener>org.seasar.teeda.core.event.PhaseChangeTracer</phase-listener>
	</lifecycle>

	<managed-bean>
		<managed-bean-name>helloBean</managed-bean-name>
		<managed-bean-class>examples.teeda.dto.HelloBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>commandLinkBean</managed-bean-name>
		<managed-bean-class>examples.teeda.action.CommandLinkBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
